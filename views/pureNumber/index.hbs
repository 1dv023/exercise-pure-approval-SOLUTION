<h2>Pure Numbers</h2>

{{#if pureNumbers }}
<table class="table table-hover">
  <caption>List of pure numbers</caption>
  <thead class="thead-light">
    <tr>
      <th scope="col">Created</th>
      <th scope="col">Value</th>
    </tr>
  </thead>
  <tbody>
    {{#each pureNumbers}}
    <tr>
      <th scope="row">{{this.createdAt}}</th>
      <td>{{this.value}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
{{/if}}